<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <directoryBrowse enabled="false" />
        <rewrite>
            <rules>
                <rule name="Plesk. Wordpress redirect wpConfigRule #00f18c43-708f-43e7-86e8-5143effeb3fe" stopProcessing="true">
                    <match url="^wp-config.php$" ignoreCase="false" />
